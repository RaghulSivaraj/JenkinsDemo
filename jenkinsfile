pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building'
            }
        }
        stage('Deploying') {
            steps {
                echo 'Deployng'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
        stage('Release') {
            steps {
                echo 'Releasing'
            }
        }
        stage('Demo') {
            steps {
                git branch: 'main', credentialsId: 'MyGithub', url: 'https://github.com/Rahul-designs/JenkinsDemo.git'
            }
        }
    }
}
